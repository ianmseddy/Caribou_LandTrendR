}


//turn this filter into a function that will filter by docID - or is this relevant.. do we just need the geographic area to avoid double counting. 

var provincialFilt = ee.Filter.inList("Province", ["SK", "MB"]);
var FilteredAOI = aoi.filter(provincialFilt);
// filter using a function - we will use this with the map function to run LandTrendr for each study 
function filterByDocID(DocID){
  return(ee.Filter.inList("DocID", DocID))
}
